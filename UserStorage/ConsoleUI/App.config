<?xml version="1.0" encoding="utf-8" ?>
<configuration>

  <configSections>
    <section name="Services" type="Configurator.ReplicationInfo.ServicesConfigSection, Configurator"/>
  </configSections>
  <Services>
      <Master count="1" />
      <Slave count="3" />
  </Services>
  
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  
  <system.diagnostics>
    <trace autoflush="true"/>
    <sources>
      <source name="traceSource"
              switchName="traceSourceSwitch"
              switchType="System.Diagnostics.SourceSwitch" >
        <listeners>            
          <add name="textwriterListener"
            type="System.Diagnostics.TextWriterTraceListener"
            initializeData="out.txt"/>
          <add name="xmlListener"
            type="System.Diagnostics.XmlWriterTraceListener"
            initializeData="outfile.xml"
            traceOutputOptions="DateTime" />
        </listeners>
      </source>
    </sources>
    <switches>
      <add name="traceSwitch" value="0"/>
      <add name="traceSourceSwitch" value="Information" />
    </switches>
  </system.diagnostics>

    <appSettings>
        <add key="fileName" value="users.xml" />
    </appSettings>



</configuration>